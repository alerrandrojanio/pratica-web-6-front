\r\n<div>\r\n\r\n\r\n  <div class=\"campos\">\r\n    <h4>Criar Post</h4>\r\n\r\n           <p> Id: <input type=\"text\" v-model=\"id\" /><br /></p> \r\n           <p> Corpo: <input type=\"text\" v-model=\"corpo\" /><br /></p> \r\n           <p> Data: <input type=\"text\" v-model=\"data\" /><br /></p>\r\n           <p> Hora: <input type=\"text\" v-model=\"hora\" /><br /></p>\r\n           \r\n   </div>\r\n\r\n  <div class=\"butaos\">\r\n    <button @click=\"post()\">Publicar</button><br /><br />\r\n    <button @click=\"put()\">Editar</button><br /><br />\r\n    <button @click=\"delet()\">Deletar</button><br /><br /> \r\n  </div>\r\n\r\n   <ul>\r\n      <li>\r\n        <div class=\"infor\">\r\n          <p> Id: {{ post.id }}</p>\r\n          <p>Corpo: {{ post.corpo }}</p>\r\n          <p>Data: {{ post.data }}</p>\r\n          <p>Hora: {{ post.hora }}</p>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n  data() {\r\n    return {\r\n      id: 0,\r\n      corpo: \"\",\r\n      data: \"\",\r\n      hora:\"\",\r\n      posts: [],\r\n      baseURI: \"http://localhost:3000/posts\"\r\n    }\r\n  },\r\n  \r\n  methods: {\r\n    post(){\r\n      let post ={\r\n        corpo: this.corpo,\r\n        data: this.data,\r\n        hora: this.hora\r\n      };\r\n\r\n      axios.post(this.baseURI, post).then((resul) =>{\r\n        this.posts = resul.data\r\n      })\r\n    },\r\n\r\n    put(){\r\n      let post ={\r\n        corpo: this.corpo,\r\n        data: this.data,\r\n        hora: this.hora\r\n      };\r\n\r\n      axios.put(this.baseURI + \"/\" + this.id, post).then((result) =>{\r\n        console.log(result)\r\n      })\r\n    },\r\n\r\n    delet(){\r\n      axios.delete(this.baseURI + \"/\" + this.id).then((result) =>{})\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n.Campos {\r\n    margin: auto;\r\n    font-weight: 600;\r\n    width: 40%;\r\n        \r\n}\r\n.butaos{\r\n    display: flex;\r\n    width: 70%;\r\n    justify-content: center;\r\n    margin: 10px auto;\r\n  \r\n}\r\n.butaos button{\r\n    background-color: rgb(23, 91, 192);\r\n    color: white;\r\n    margin: 0 20px;\r\n    cursor: pointer;\r\n}\r\n.infor {\r\n    display: flex;\r\n}\r\n.infor p {\r\n    margin: 10px 10px;\r\n    \r\n}\r\n\r\n\r\n</style>"]}]}